#include<stdio.h>
main()
{
    int choice=0;
	printf(" Enter a number from among the following 1,2,3,4,5. \n");
	scanf(" %d",&choice);
	switch(choice)
	{
		case 1:
			printf(" Buy a Pizza for Rs 239");
			break;
		case 2:
			printf(" Buy a Burger for Rs 129");
			break;
		case 3:
			printf(" Buy Pasta for Rs 179");
			break;
		case 4:
			printf(" Buy French fries for Rs 99");
			break;
		case 5:
			printf(" Buy a Sandwich for Rs 149");
			break;
		default:
		    printf("Invalid choice");	
	}
	return 0;
}
