#include<stdio.h>
main()
{
	float X;
	printf("Enter the marks of the student out of 100 \n");
	scanf("%f",&X);
	if(X>85 || X==85)
    printf("\n A");
    else if(X>70 || X==70)
    printf(" \n B");
    else if(X>55 || X==55)
    printf(" \n C");
    else if(X>40 || X==40)
    printf("\n D");
    else if(X>0|| X==0)
    printf("\n F");
    return 0;
}
